{
    "name": "satnin/bad-word-filter",
    "description": "PHP Bad word filtering. Checks for the existence of a bad word in a string or array.",
    "license": "MIT",
    "authors": [
        {
            "name": "Saturnin Wanonkou",
            "email": "saturninwanonkou17@gmail.com"
        }
    ],
    "require": {
        "php": "8.*",
        "illuminate/support": ">=4.0.0"
    },
    "require-dev": {
      "phpunit/phpunit": "4.5.1",
      "mockery/mockery": "0.9.*",
      "satooshi/php-coveralls": "1.0.1"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "JCrowe\\BadWordFilter\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
